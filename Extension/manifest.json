{
  "name": "Gitmoji Extension",
  "version": "1.0",
  "description": "Easily search and copy gitmojis ðŸš€",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_title": "Gitmoji Extension",
    "default_icon": "./assets/popup-icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./assets/popup-icon16.png",
    "48": "./assets/popup-icon48.png",
    "128": "./assets/popup-icon128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Shift+Alt+G"
      },
      "description": "Open the gitmoji popup"
    }
  },
  "manifest_version": 2
}
